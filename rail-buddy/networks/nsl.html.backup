<style>
    .line-diagram {
        padding: 40px 20px;
        background: #f8f9fa;
        border-radius: 12px;
        margin-bottom: 40px;
        overflow-x: auto;
    }

    .diagram-wrapper {
        position: relative;
        min-width: 1200px;
        padding: 60px 40px;
    }

    .phase-section {
        display: inline-block;
        margin-right: 30px;
        position: relative;
    }

    .phase-label {
        position: absolute;
        top: -50px;
        left: 0;
        font-size: 12px;
        font-weight: bold;
        color: #666;
        background: #fff;
        padding: 5px 10px;
        border-radius: 4px;
        text-transform: uppercase;
        letter-spacing: 1px;
    }

    body.dark-mode .phase-label {
        background: #2d3242;
        color: #ccc;
    }

    .line-track {
        display: flex;
        align-items: flex-end;
        gap: 2px;
        position: relative;
        height: 100px;
    }

    /* Horizontal connecting line */
    .line-track::before {
        content: '';
        position: absolute;
        left: 0;
        right: 0;
        top: 50%;
        height: 3px;
        background: linear-gradient(90deg, #c41e3a, #c41e3a);
        z-index: 1;
        transform: translateY(-50%);
    }

    .station {
        text-align: center;
        flex-shrink: 0;
        display: flex;
        flex-direction: column;
        align-items: center;
        position: relative;
        min-width: 65px;
    }

    .station-code {
        width: 38px;
        height: 38px;
        border-radius: 50%;
        background: #c41e3a;
        color: white;
        display: flex;
        align-items: center;
        justify-content: center;
        font-weight: bold;
        font-size: 11px;
        z-index: 10;
        border: 2px solid #fff;
        box-shadow: 0 2px 6px rgba(0, 0, 0, 0.15);
        margin-bottom: 12px;
    }

    body.dark-mode .station-code {
        border-color: #1c1f2b;
    }

    .station-name {
        font-size: 10px;
        font-weight: 600;
        white-space: nowrap;
        color: #333;
        margin-top: auto;
        margin-bottom: 8px;
    }

    body.dark-mode .station-name {
        color: #ccc;
    }

    .station.future .station-code {
        background: #f0f0f0;
        color: #999;
        border: 2px dashed #c41e3a;
        font-style: italic;
    }

    .interchanges {
        display: flex;
        gap: 4px;
        flex-wrap: wrap;
        justify-content: center;
        min-height: 24px;
        margin-top: 8px;
    }

    .interchange-badge {
        font-size: 9px;
        font-weight: bold;
        padding: 2px 6px;
        border-radius: 10px;
        color: white;
        white-space: nowrap;
        text-shadow: 0 1px 1px rgba(0, 0, 0, 0.2);
    }

    /* Line colors */
    .mrt-ewl { background: #009645; }
    .mrt-ccl { background: #fa7921; }
    .mrt-tel { background: #926437; }
    .mrt-dtl { background: #005ec8; }
    .mrt-nel { background: #8b2e6b; }
    .mrt-cel { background: #f3c100; color: #333; text-shadow: none; }
    .mrt-bp { background: #7f7f7f; }
    .mrt-cr { background: #6b4c9a; }
    .mrt-js { background: #d4a241; }
    .mrt-je { background: #d4a241; }
    .mrt-de { background: #4ba3d4; }

    /* Terminal stations styling */
    .station.terminal .station-code {
        border: 3px solid #c41e3a;
    }

    /* Responsive */
    @media (max-width: 768px) {
        .diagram-wrapper {
            min-width: auto;
        }

        .line-track {
            gap: 1px;
            height: 80px;
        }

        .station {
            min-width: 55px;
        }

        .station-code {
            width: 32px;
            height: 32px;
            font-size: 10px;
            margin-bottom: 10px;
        }

        .station-name {
            font-size: 8px;
            margin-bottom: 6px;
        }

        .phase-label {
            font-size: 10px;
            top: -40px;
        }
    }

    body.dark-mode .line-diagram {
        background: #2d3242;
    }
</style>

